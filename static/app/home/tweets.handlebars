{{#each statuses}}
  <div class="col l12">
    <div class="card blue-grey darken-1">
      <div class="card-content white-text">
        <span class="card-title">
          <a href="http://twitter.com/{{user.screen_name}}">
            @{{user.screen_name}}
          </a>
        </span>
        <p>{{text}}</p>
        <p>{{sentiment}}</p>
      </div>
      <div class="card-action">
        <a href="#">More detail</a>
      </div>
    </div>
  </div>
{{else}}
  <p>nada</p>
{{/each}}

